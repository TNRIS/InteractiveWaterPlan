<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>

    <link href="@Url.Content("~/Content/ExtJS/resources/css/ext-all-gray.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/OpenLayers/theme/default/style.css")" rel="stylesheet" type="text/css"/>
    <link href="@Url.Content("~/Content/bootstrap/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />

    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />

   

</head>

<body>

    @RenderBody()

    @*TODO: Bundle the scripts *@
    <input type="hidden" id="scripts_path" value="@Url.Content("~/Scripts")" />
    @if (HttpContext.Current.IsDebuggingEnabled)
    {
        <script src="@Url.Content("~/Content/OpenLayers/OpenLayers.debug.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Content/ExtJS/ext-all-debug.js")" type="text/javascript"></script>
    }
    else
    {
        <script src="@Url.Content("~/Content/OpenLayers/OpenLayers.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Content/ExtJS/ext-all.js")" type="text/javascript"></script>
    }
    
    <script type="text/javascript" src="@Url.Content("~/Content/OpenLayers/tile.stamen.js")"></script>
    <script src="@Url.Content("~/Scripts/Compiled/OpenLayers/OpenLayers.Layer.QuadKey.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Compiled/OpenLayers/OpenLayers.Control.GetFeature.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Compiled/app.js")" type="text/javascript"></script>
    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
</body>
</html>
