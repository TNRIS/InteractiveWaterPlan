@{
    ViewBag.Title = "Error";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section MenuLinks {
    <a href="@Url.Action("Index", "Home")" title="Back to Viewer" class="btn">Back to Viewer</a>            
}

<div class="row">
    <div class="span9">
        <h1 class="page-title">Server Error</h1>
        <p>We're sorry, an unexpected error has occurred and has been logged.</p>  
        <p>
            If the the problem continues, please <a href="mailto:InteractiveSWP@twdb.texas.gov" title="Contact Us">contact us</a>
            with a description of what you were doing when the error occurred. 
            Please also include your operating system name and version
            as well as your Internet browser name and version.
        </p>
        <p>
            Return to the <a href="@Url.Action("Index", "Home")" title="Main Page">main page</a> 
            or visit the <a href="http://twdb.texas.gov" title="TWDB Home">Texas Water Development Board's 
            home page</a>.
        </p>
          
    </div>
</div>


