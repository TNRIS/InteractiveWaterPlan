<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="InteractiveWaterPlan.Core" namespace="InteractiveWaterPlan.Core">

  <class name="StrategyType" mutable="false">
    <id name="Id">
      <column name="WMSStrategy_ID"/>
    </id>

    <property column="WMSStrategy_Type" name="Name" />
  </class>

  <sql-query name="GetAllStrategyTypes">
    <return class="StrategyType"/>
    exec dbo.usp_Select_ALL_StrategyTypes__tbl_SWP_Final
  </sql-query>

</hibernate-mapping>