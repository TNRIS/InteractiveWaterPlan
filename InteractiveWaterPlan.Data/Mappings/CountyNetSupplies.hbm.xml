<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="InteractiveWaterPlan.Core" namespace="InteractiveWaterPlan.Core">

  <class name="CountyNetSupply" mutable="false">
    
    <id name="CountyId">
      <column name="County_ID"/>
    </id>

    <property column="County_Name" name="CountyName" />
    
    <property column="Region_Letter" name="RegionLetter" />
    
    <property column="MUNICIPAL" name="NetMunicipal" />
    <property column="IRRIGATION" name="NetIrrigation" />
    <property column="MANUFACTURING" name="NetManufacturing" />
    <property column="LIVESTOCK" name="NetLivestock" />
    <property column="STEAM_ELECTRIC" name="NetSteamElectric" />
    <property column="MINING" name="NetMining" />

  </class>

  <sql-query name="GetCountyNetSupplies">
    <query-param name="year" type="string"/>
    <return class="CountyNetSupply"/>
    exec dbo.usp_Select_CountySum_tbl_SWP_Final_By_Year :year
  </sql-query>

</hibernate-mapping>