<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="InteractiveWaterPlan.Core" namespace="InteractiveWaterPlan.Core">

  <class name="County" mutable="false">
    <id name="Id">
      <column name="SQL_ID"/>
    </id>

    <property column="COUNTY" name="Name" />
    <property column="SQL_GEOG" name="WktGeog" />
  </class>

  <sql-query name="GetAllCounties">
    <return class="County"/>
    exec dbo.usp_Select_ALL_tbl_Bndry_Counties
  </sql-query>

</hibernate-mapping>