<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="InteractiveWaterPlan.Core" namespace="InteractiveWaterPlan.Core">

  <class name="Reservoir" mutable="false">
    <id name="Id">
      <column name="SQL_ID"/>
    </id>

    <property column="DB12_Id" name="ReservoirId" />
    <property column="Reservoir_Name" name="Name" />
    <property column="Recommendation_Code" name="RecommendationCode" />
    <property column="SQL_GEOG" name="WktGeog" />

  </class>

  <sql-query name="GetAllRecommendedReservoirs">
    <return class="Reservoir"/>
    exec dbo.usp_Select_ALL_tbl_GEMSS_Vector_TWDB_DB12_Recommended_Reservoirs :var_SimplifyTolerance
  </sql-query>

  <sql-query name="GetReservoirInBufferedPoint">
    <return class="Reservoir"/>
    exec dbo.usp_Select_tbl_GEMSS_Vector_TWDB_DB12_Recommended_Reservoirs :var_PointGeog, :var_BufferPoly, :var_SimplifyTolerance
  </sql-query>
  
</hibernate-mapping>