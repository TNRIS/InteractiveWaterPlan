Map {
  background-color: transparent;
}

#rwpas {
  line-color:#594;
  line-width:0.5;
  polygon-opacity:1;
  polygon-fill:#ae8;

  [LETTER = "A"] { polygon-fill: rgba(116, 196, 118, 0.6); }
  [LETTER = "B"] { polygon-fill: rgba(0, 169, 230, 0.6); }
  [LETTER = "C"] { polygon-fill: rgba(40, 79, 176, 0.6); }
  [LETTER = "D"] { polygon-fill: rgba(102, 119, 205, 0.6); }
  [LETTER = "E"] { polygon-fill: rgba(161, 217, 155, 0.6); }
  [LETTER = "F"] { polygon-fill: rgba(198, 219, 239, 0.6); }
  [LETTER = "G"] { polygon-fill: rgba(224, 243, 219, 0.6); }
  [LETTER = "H"] { polygon-fill: rgba(102, 153, 205, 0.6); }
  [LETTER = "I"] { polygon-fill: rgba(168, 221, 181, 0.6); }
  [LETTER = "J"] { polygon-fill: rgba(64, 93, 189, 0.6); }
  [LETTER = "K"] { polygon-fill: rgba(108, 125, 212, 0.6); }
  [LETTER = "L"] { polygon-fill: rgba(204, 235, 197, 0.6); }
  [LETTER = "M"] { polygon-fill: rgba(9, 62, 153, 0.6); }
  [LETTER = "N"] { polygon-fill: rgba(195, 197, 247, 0.6); }
  [LETTER = "O"] { polygon-fill: rgba(0, 132, 168, 0.6); }
  [LETTER = "P"] { polygon-fill: rgba(8, 104, 172, 0.6); }
}
