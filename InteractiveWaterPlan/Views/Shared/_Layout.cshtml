<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>

    <link href="@Url.Content("~/Content/ExtJS/resources/css/ext-all-gray.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/OpenLayers/theme/default/style.css")" rel="stylesheet" type="text/css"/>
    <link href="@Url.Content("~/Content/bootstrap/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />

    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />

    @if (HttpContext.Current.IsDebuggingEnabled)
    {
        <script src="@Url.Content("~/Content/OpenLayers/OpenLayers.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Content/ExtJS/ext-all-debug.js")" type="text/javascript"></script>
    }
    else
    {
        <script src="@Url.Content("~/Content/OpenLayers/OpenLayers.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Content/ExtJS/ext-all.js")" type="text/javascript"></script>
    }
    
    
    <script src="@Url.Content("~/Scripts/Compiled/app.js")" type="text/javascript"></script>
    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

</head>

<body>
    @RenderBody()
</body>
</html>
